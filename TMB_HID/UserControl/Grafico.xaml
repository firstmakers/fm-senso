            <Setter Property="IsTabStop" Value="False"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type chartingToolkit:LegendItem}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="0*"/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>
                            <Border Grid.Row="1" BorderBrush="#FF2B6097" BorderThickness="1" Padding="0" CornerRadius="12.5" Grid.RowSpan="2" Background="#FF55A9E9">
                                <StackPanel Orientation="Horizontal">
                                    <Grid Width="25" Height="25" >
                                        <Rectangle Fill="White" RadiusX="12,5" RadiusY="12,5" Stroke="#FF2B6097"/>
                                        <Rectangle Fill="{Binding Background}" Height="8"  Margin="0,0,0,0" Stroke="{Binding BorderBrush}" StrokeThickness="1" Width="8"/>
                                    </Grid>
                                    <visualizationToolkit:Title Content="{TemplateBinding Content}" Foreground="White" HorizontalContentAlignment="Center" HorizontalAlignment="Center" VerticalAlignment="Center"  FontSize="13.333" Margin="5,0,0,0" FontFamily="/TMB_HID;component/Fonts/#Ubuntu Titling Rg"/>
                                </StackPanel>
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <!--Fin Style humedad-->
    </UserControl.Resources>

    <Grid>
        <chartingToolkit:Chart VerticalAlignment="Bottom" VerticalContentAlignment="Bottom"  x:Name="lineChart"  Margin="2,0,2,0" Style="{DynamicResource ChartStyleHum}" LegendStyle="{DynamicResource LegendStyleTMB}" TitleStyle="{DynamicResource TitleStyle}"  >
            <chartingToolkit:LineSeries  x:Name="mSerie"   DependentValuePath="Value" IndependentValuePath="Key" ItemsSource="{Binding ValueList}" IsSelectionEnabled="True"  LegendItemStyle="{DynamicResource LegendItemStyleHum}" Margin="0,-7.333,0,0.667"  Style="{DynamicResource LineSeriesStyle}" DataPointStyle="{DynamicResource LineDataPointStyle}">
                <chartingToolkit:LineSeries.IndependentAxis>
                    <chartingToolkit:CategoryAxis
                        Orientation="X">
                        <chartingToolkit:CategoryAxis.AxisLabelStyle>
                            <Style TargetType="chartingToolkit:AxisLabel">

                                <Setter Property="Template">
                                    <Setter.Value>
                                        <ControlTemplate TargetType="chartingToolkit:AxisLabel">
                                            <TextBlock Text="{TemplateBinding FormattedContent}">
                                                <TextBlock.LayoutTransform>
                                                    <RotateTransform Angle="-60"/>
                                                </TextBlock.LayoutTransform>
                                            </TextBlock>
                                        </ControlTemplate>
                                    </Setter.Value>
                                </Setter>
                            </Style>
                        </chartingToolkit:CategoryAxis.AxisLabelStyle>
                    </chartingToolkit:CategoryAxis>
                </chartingToolkit:LineSeries.IndependentAxis>
            </chartingToolkit:LineSeries>
        </chartingToolkit:Chart>

        <StackPanel  Orientation="Vertical" Margin="0,61,27,77" HorizontalAlignment="Right" Width="129">
            <Label x:Name="VP" HorizontalContentAlignment="Center" FontSize="24" Padding="0" VerticalAlignment="Top" Margin="0,8,0,0"    Height="22" FontFamily="/TMB_HID;component/Fonts/#DS-Digital"/>
            <Label x:Name="VS" HorizontalContentAlignment="Center"  FontSize="15" Padding="0" VerticalAlignment="Top" Margin="0,5,0,0" FontFamily="/TMB_HID;component/Fonts/#DS-Digital"   />
        </StackPanel>

    </Grid>
</UserControl>
